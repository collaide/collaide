<h1>Listing member_group_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Is public</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @member_group_groups.each do |member_group_group| %>
  <tr>
    <td><%= member_group_group.name %></td>
    <td><%= member_group_group.description %></td>
    <td><%= member_group_group.is_public %></td>
    <td><%= member_group_group.password %></td>
    <td><%= link_to 'Show', member_group_group %></td>
    <td><%= link_to 'Edit', edit_member_group_group_path(member_group_group) %></td>
    <td><%= link_to 'Destroy', member_group_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

