<h1>Listing member_statuses</h1>

<table>
  <tr>
    <th>Message</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @member_statuses.each do |member_status| %>
  <tr>
    <td><%= member_status.message %></td>
    <td><%= link_to 'Show', member_status %></td>
    <td><%= link_to 'Edit', edit_member_status_path(member_status) %></td>
    <td><%= link_to 'Destroy', member_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Status', new_member_status_path %>
