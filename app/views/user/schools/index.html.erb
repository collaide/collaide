<h1>Listing user_schools</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_schools.each do |user_school| %>
  <tr>
    <td><%= user_school.name %></td>
    <td><%= user_school.description %></td>
    <td><%= link_to 'Show', user_school %></td>
    <td><%= link_to 'Edit', edit_user_school_path(user_school) %></td>
    <td><%= link_to 'Destroy', user_school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New School', new_user_school_path %>
