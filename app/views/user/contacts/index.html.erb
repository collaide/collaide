<h1>Listing user_contacts</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Date of birth</th>
    <th>Gender</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_contacts.each do |user_contact| %>
  <tr>
    <td><%= user_contact.first_name %></td>
    <td><%= user_contact.last_name %></td>
    <td><%= user_contact.date_of_birth %></td>
    <td><%= user_contact.gender %></td>
    <td><%= link_to 'Show', user_contact %></td>
    <td><%= link_to 'Edit', edit_user_contact_path(user_contact) %></td>
    <td><%= link_to 'Destroy', user_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_user_contact_path %>
