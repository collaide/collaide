<h1>Listing user_addresses</h1>

<table>
  <tr>
    <th>Country</th>
    <th>Street</th>
    <th>Street number</th>
    <th>City code</th>
    <th>Country code</th>
    <th>Is actual</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_addresses.each do |user_address| %>
  <tr>
    <td><%= user_address.country %></td>
    <td><%= user_address.street %></td>
    <td><%= user_address.street_number %></td>
    <td><%= user_address.city_code %></td>
    <td><%= user_address.country_code %></td>
    <td><%= user_address.is_actual %></td>
    <td><%= link_to 'Show', user_address %></td>
    <td><%= link_to 'Edit', edit_user_address_path(user_address) %></td>
    <td><%= link_to 'Destroy', user_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Address', new_user_address_path %>
